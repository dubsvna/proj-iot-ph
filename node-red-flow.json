    {
        "id": "55f407ce0e32b602",
        "type": "function",
        "z": "c105a06c16844480",
        "name": "Alerta Telegram",
        "func": "// Configura√ß√µes\nconst PH_MIN = 6.0;\nconst PH_MAX = 8.0;\nlet alertaAtivo = false;\nlet normalizadorAtivo = false;\n\n// Valor do pH e Nome do Equipamento vindo do payload\nconst ph = parseFloat(msg.payload.ph);\nconst equipamento = msg.payload.sensor_id\nconst temp = msg.payload.temp\nif (isNaN(ph)) {\n    node.error(\"Valor de pH inv√°lido\");\n    return null;\n}\nmsg.payload.chatId = '@guapos_iot'\nmsg.payload.content = ''\nmsg.payload.type = 'message'\n// Verifica limites - pH DESREGULADO\nif (ph < PH_MIN || ph > PH_MAX) {\n    if (!alertaAtivo) {\n        // Cria mensagem de alerta\n        let tipo = ph < PH_MIN ? \"√Åcido\" : \"Alcalino\";\n        let limite = ph < PH_MIN ? PH_MIN : PH_MAX;\n        msg.payload.content = `üö® ALERTA - pH DESREGULADO!\\n` +\n            `Nome: ${equipamento.toUpperCase()}\\n` +\n            `Valor atual: ${ph.toFixed(2)}\\n` +\n            `Limite ideal: ${limite}\\n` +\n            `Temperatura do Recipiente: ${temp}\\n` +\n            `Tipo: ${tipo}\\n` +\n            `üíß Normalizador sendo inserido na √°gua`;\n        alertaAtivo = true;\n        normalizadorAtivo = true;\n        return msg;\n    }\n} else {\n    // pH REGULADO (dentro dos limites normais)\n    if (alertaAtivo || normalizadorAtivo) {\n        msg.payload.content = `‚úÖ pH REGULADO - SISTEMA NORMAL\\n` +\n            `Nome: ${equipamento.toUpperCase()}\\n` +\n            `Valor atual: ${ph.toFixed(2)}\\n` +\n            `Situa√ß√£o: Dentro dos limites ideais\\n` +\n            `‚èπÔ∏è Normalizador desativado`;\n\n        alertaAtivo = false;\n        normalizadorAtivo = false;\n        return msg;\n    }\n}",
